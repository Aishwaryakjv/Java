equals():
if(s1.equals(s2))
System.out.print("equal");

equalsIgnoreCase():
if(s1.equalsIgnoreCase(s2))
System.out.print("equal");

replace(char c1,char c2):
System.out.print(s1.replace(ch1,ch2));

charAt():
System.out.print(s1.charAt(position));

toUpperCase():
System.out.print(s1.toUpperCase());

substring(int,int): //note: start index is inclusive and end index is exclusive
System.out.print(s1.substring(0,3));
